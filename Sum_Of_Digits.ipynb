   "source": [
    "## Calculate sum of digits in a given number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sumDigit(num):\n",
    "    cal_sum = 0\n",
    "    while(num>0):\n",
    "        rem = num % 10\n",
    "        cal_sum = cal_sum + rem\n",
    "        num = num//10\n",
    "    return cal_sum"
   ]
  },
